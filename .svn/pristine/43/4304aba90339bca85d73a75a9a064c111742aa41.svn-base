.bry_age{
	display: inline-block;
}
.bry_age>input{
	width: 154px;
	height: 40px;
	border: 1px solid #059fbf;
	border-radius: 7px;
	padding-left: 8px;
	box-sizing: border-box;
	outline: none;
	margin-left: 4px;
	margin-right: 8px;
}
.class_w{
	padding-left: 27px;
}
.reage>input{
	width: 54px;
}
.reage>select{
	width: 64px;
	height: 40px;
	border: 1px solid #059fbf;
	border-radius: 7px;
	padding-left: 0px;
	box-sizing: border-box;
	outline: none;
	margin-left: 14px;
}
.reage{
	margin-right: 40px;
}
.bry_industry_top_rig{
	font-size: 14px;
	color: #333;
}
.bry_industry_nation{
	padding-bottom: 0;
}
.bry_industry_top_rig>input{
	margin-left: 14px;
}
.bry_industry_top_rig>span{
	margin-left: 4px;
	vertical-align: baseline;
}
.bry_industry_le_top{
	height: 80px;
	margin-bottom: 20px;
}
.bry_industry_num{
	position: relative;
	padding-left: 12px;
}
.bry_industry_nation{
	padding-left: 12px;
	padding-top: 0;
	margin-bottom: 20px;
	margin-left: 60px;
}
.bry_industry_nation div{
	display: inline-block;
}
.bry_span_num{
	width: 360px;
	height: 40px;
	padding-right: 4px;
}
/* 背景显示正面和背面 */
.bry_bg1{
	background: url(../../assets/icon/facade.png) no-repeat  center;
	background-size: 100% 100%;
	background-color: #fff;
}
.bry_bg2{
	margin-left: 20px;
	background: url(../../assets/icon/back.png) no-repeat  center;
	background-size: 100% 100%;
	background-color: #fff;
}
.renew{
	width: 714px;
	position: relative;
}
.bry_bot_notice{
	display: inline-block;
	position: absolute;
	width: 160px;
	height: 60px;
	right: 240px;
	top: 40px;
}
.upst1{
	margin-left: 0!important;
}
.upst2{
	margin-left: 176px !important;
}

.renew>textarea{
	text-align: center;
}
.renew>textarea:nth-of-type(2){
	margin-left: 20px;
	margin-right: 100px;
}
.renew>textarea::-webkit-input-placeholder{
      line-height: 100px;
	  text-align: center;
 } 
.goleft{
	display: inline-block;
	margin-left: 60px;
}

