<script type="text/JavaScript" charset="UTF-8">
		var vueobj = new Vue({
			el: '#app',
			data(){
				return{
					topNav: ["视频", "音频", "资讯", "百科", "电子书"],
					currentIndex: 0,
					UserId: '',
					ColId: "3",
					commentData: [],  //评论数据
					username: ''  ,
					PageSize:8  ,//每页显示多少条数据
					PageNum: 1   ,//当前第几页  首次打开第一页
					total:"",    //总条数
					RoleId: '',     // 角色id
				}
			},
			created() {
				this.UserId = foowwLocalStorage.getLocal("urseId")
				this.RoleId = foowwLocalStorage.getLocal("RoleId")
				//ajax请求昵称
				getAjax("get","Users/DetailDo",'{"UserId":"' + this.UserId + '"}').then(res=>{
					this.username=res.data.NickName
				},err=>{
					console.log(err)
				})
				getAjax("get", "ArticleComment/MyCommentDo", '{"PageSize":"' + this.PageSize + '","PageNum":"' + this.PageNum + '","UserId":"' + this.UserId + '","ColId":"' + this.ColId + '"}').then(res => {
					console.log(res)
					this.total=res.other
					this.commentData=res.data
					console.log(this.total)
				}, rej => {
					console.log(rej)
				})
			},
			computed: {
				comClass(){
					return this.RoleId == 1 ? "个人" : this.RoleId == 2 ? "学生" : this.RoleId == 3 ? "讲师" : this.RoleId == 4 ? "医师" : this.RoleId == 5 ? "心里资讯师" : "机构"
				}
			},
			methods: {

				// 解码
				htmlDecode(str) {
					var s = "";
					if (str.length == 0) return "";
					s = str.replace(/&amp;/g, "&");
					s = s.replace(/&lt;/g, "<");
					s = s.replace(/&gt;/g, ">");
					s = s.replace(/&nbsp;/g, " ");
					s = s.replace(/&#39;/g, "\'");
					s = s.replace(/&quot;/g, "\"");
					return s;
				},
				// tab 切换
				changeIndex(index) {
					if(this.currentIndex == index) {
						return
					}
					this.currentIndex = index
					switch (index) {
						case 0:
							this.ColId = 3
							break
						case 1:
							this.ColId = 4
							break
						case 2:
							this.ColId = 2
							break
						case 3:
							this.ColId = 6
							break
						case 4:
							this.ColId = 7
							break
					}
					this.PageNum=1
					getAjax("get", "ArticleComment/MyCommentDo", '{"PageSize":"' + this.PageSize + '","PageNum":"' + this.PageNum + '","UserId":"' + this.UserId + '","ColId":"' + this.ColId + '"}').then(res => {
						console.log(this.ColId)
						console.log(res)
						this.total=res.other
						this.commentData=res.data
					}, rej => {
						console.log(rej)
					})
				},

				// 点击上一页
			prevClick() {
