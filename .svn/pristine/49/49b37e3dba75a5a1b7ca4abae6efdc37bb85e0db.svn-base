.bry_main {
	margin: 106px 0 0 20px;
	background-color: #fff;
	border-radius: 10px;
	width: 996px;
	height: 682px;
	padding: 40px 0 0 58px;
	box-sizing: border-box;
}

.choice {
	color: #059fbf;
}

.bry_tab {
	padding-bottom: 40px;
}

.bry_tab>li {
	float: left;
	font-size: 18px;
	cursor: pointer;
}

.bry_tab>li:nth-of-type(2) {
	margin-left: 118px;
}

.count1 {
	font-size: 14px;
	color: #333;
}

select {
	/*清除select默认样式*/
	/* appearance:none;
-moz-appearance:none;
-webkit-appearance:none;
-ms-appearance:none; */
	border: 1px solid #a0a0a0;
	padding-left: 8px;
	width: 172px;
	height: 32px;
	border-radius: 10px;
	outline: none;
	background-color: #efefef;
	color: #999;
}
.count1{
	padding-top: 26px;
}
.count1>span{
	margin-left: 20px;
}
.count2{
	padding-top: 34px;
}
.count2>input{
	width: 280px;
	height: 36px;
	border-radius: 10px;
	background-color:#efefef ;
	border: 1px  solid #a0a0a0;
	outline: none;
	padding-left: 8px;
}
.count2>span{
	margin-left: 42px;
}
.count3{
	padding-top: 42px;
}
.count3>input{
	width: 144px;
	height: 34px;
	border-radius: 10px;
	background-color:#efefef ;
	border: 1px  solid #a0a0a0;
	outline: none;
	padding-left: 8px;
}
.count3>span:nth-of-type(1){
	margin-left: 42px;
}
.shenqin{
	width: 274px;
	height:40px ;
	line-height: 40px;
	background-color: #fd9441;
	border-radius: 10px;
	margin-left: 90px;
	margin-top: 116px;
	font-size: 18px;
	color: #fff;
	outline: none;
	border: 1px solid #a0a0a0;
	cursor: pointer;
}
/* 弹出层 */
.bry_layer{
	position: fixed;
	top: 50%;
	left: 50%;
	width: 940px;
	height: 310px;
	margin-left:-470px;
	margin-top:-255px;
	border: 2px solid #1ca7c4;
	border-radius: 10px;
	background: rgba(255,255,255,0.8);
	padding: 40px 100px 0 62px;
	box-sizing: border-box;
}

.bry_success_pic{
	width: 50px;
	height: 50px;
	border-radius: 50%;
	/* float: left; */
}
.bry_success_pic>img{
	width: 100%;
	height: 100%;
	border-radius: 50%;
}
.bry_success_word{
	width: 700px;
	/* float: left; */
	margin-left: 20px;
}
.bry_success_word>span{
	display: block;
	color: #333333;
}
.bry_success_word>span:first-child{
	font-size: 18px;
	font-weight: bold;
}
.bry_success_word>span:last-child{
	font-size: 14px;
}

/* 选项内容 */
.bry_option_content{
	width: 880px;
}
.bry_option_content>li{
	width: 100%;
	padding-left: 16px;
	margin-top: 16px;
	font-size: 12px;
	box-sizing: border-box;
	background: url(../../../assets/icon/title-pic1.png) no-repeat left  center;
	background-size: 14px 14px;
}
.bry_option_content>li>div{
	display: inline-block;
	padding-bottom: 2px;
	width: 100%;
	border-bottom: 1px dashed #666666;
	box-sizing: border-box;
	color: #666666;
}
.bry_option_content>li>div>span:first-child{
	float: left;
	width: 460px;
	overflow: hidden;
}
.bry_option_content>li>div>span:last-child{
	float: right;
	width: 120px;
}