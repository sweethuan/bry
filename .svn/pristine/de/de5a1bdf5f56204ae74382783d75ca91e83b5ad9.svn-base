body {
    background: #059fbf;
    width: 100%;
    height: 100%;
}

.bry_container {
    width: 1320px;
    margin: 0 auto;
}

/* 标题及退出登录 */
.bry_title_logout {
    line-height: 30px;
    padding-top: 35px;
    padding-bottom: 18px;
}

.bry_title_logout>p:nth-of-type(1) {
    font-size: 20px;
    font-family: "Source Han Sans CN", "微软雅黑";
    color: #efefef;
    box-sizing: border-box;;
    padding-left: 36px;
    background: url('../../assets/icon/mechanism/mechan_head.png') no-repeat left 7px center;
    background-size: 22px 22px;
}
.bry_title_logout>p:nth-of-type(2) {
    font-size: 14px;
    color: white;
    padding-right: 50px;
}
.bry_title_logout>p:nth-of-type(2)>a {
    color: #ffd132;
    margin-left: 6px;
}

/* 内容区 */
.bry_inner {
    width: 100%;
    min-height: 1098px;
    box-sizing: border-box;
    margin-bottom: 54px;
    background: #efefef;
    border-radius: 5px;
    position: relative;
}

/* 头像及侧导航 */
.bry_head_leftnav {
    width: 190px;
    padding-left: 22px;
}

/* 头像 */
.bry_headicon {
    text-align: center;
    font-size: 0;
    padding-top: 35px;
    padding-bottom: 22px;
}
.bry_headicon>div {
    width: 112px;
    height: 112px;
    border-radius: 50%;
    margin: 0 auto;
    overflow: hidden;
}
.bry_headicon>div>img {
    width: 100%;
    height: 100%;
}
.bry_headicon>p {
    font-size: 14px;
    color: #666666;
    padding-top: 12px;
}

/* 侧导航 */
.bry_sidenav {
    
}

.bry_sidenav>li {
    width: 100%;
    font-size: 14px;
    background: white;
    line-height: 26px;
    border-radius: 4px;
    box-sizing: border-box;
    /* padding-left: 60px; */

}
.bry_sidenav>li>a {
    color: #333333;
    margin-left: 60px;
}
.bry_sidenav .bry_space1 {
    margin-bottom: 4px;
}
.bry_sidenav .bry_space2 {
    margin-top: 15px;
    margin-bottom: 16px;
}
.bry_sidenav .bry_space3 {
    margin-bottom: 14px;
}
.bry_sidenav .bry_multisel {
    line-height: 30px;
}
.bry_sidenav .bry_multisel li {
    border-radius: 5px;
}
.bry_sidenav .bry_multisel>span {
    margin-left: 60px;
}
.bry_sidenav .bry_multisel>ul>li>a {
    font-size: 12px;
    color: #333333;
    margin-left: 60px;
}

/* 侧导航选中效果 */
.bry_sidenav_sel {
    background: #059fbf !important;
}
.bry_sidenav_sel a {
    color: white !important;
}