.bry_screen_MoviesList{
	margin-top: 17px;
}
.bry_screen_MoviesLi{
	float: left;
	width: 200px;
	margin-left: calc(82px / 5);
	padding-bottom: 48px;
	position: relative;
}
.bry_Pay{
	width: 49px;
	height: 28px;
	position: absolute;
	top: 0;
	left: 0;
	background: url(../../assets/icon/bry_Pay.png) no-repeat;
}
.bry_screen_MoviesLi:nth-of-type(6n+1){
	margin-left: 0;
}
.bry_screen_MoviesLi_picture{
	width: 200px;
	height: 114px;
	border-radius: 8px;
	overflow: hidden;
}
.bry_screen_MoviesLi_picture>img{
	width: 100%;
	height: 100%;
}
.bry_screen_MoviesLi_Name{
	display: block;
	width: 100%;
	padding: 5px 0;
	box-sizing: border-box;
	text-align: center;
	font-size: 14px;
	color: #FFFFFF;
}
.bry_screen_MoviesLi_lookNum{
	width: -webkit-fit-content;
	margin: 0 auto;
}
.bry_screen_MoviesLi_lookNum>img{
	display: block;
	float: left;
}
.bry_screen_MoviesLi_lookNum>span{
	display: block;
	float: left;
	margin: -2px 0 0 8px;
	font-size: 12px;
	color: #02c6d5;
}

/* 翻页 */
.bry_encyclopedias_page {
	width: 1280px;
	height: 24px;
	margin: 0 auto;
	display: flex;
	justify-content: flex-end;
	line-height: 32px;
}

.bry_encyclopedias_page_list {
	float: right;
}

.bry_encyclopedias_page_list li,
strong {
	height: 24px;
	float: left;
	color: #02c6d5;
	font-size: 14px;
	margin-left: 16px;
}

.page_button {
	height: 24px;
	color: #02c6d5;
	background: transparent;
	border: none;
	margin: 0 4px;
	cursor: pointer;
}

.page_left_arrow {
	width: 18px;
	height: 24px;
	background: url(../../assets/icon/page_left_arrow.png) no-repeat center;
}

.page_right_arrow {
	width: 18px;
	height: 24px;
	background: url(../../assets/icon/page_right_arrow.png) no-repeat center;
}

.page-item {
	width: 22px;
	height: 24px;
	color: #FFFFFF;
	background: #017289;
	border: none;
	border-radius: 5px;
}

.page-item.active {
	background: #c0831a;
}

.bry_encyclopedias_page_list input {
	width: 24px;
	height: 24px;
	background: #02c6d5;
	border-radius: 5px;
	text-align: center;
}

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
	-webkit-appearance: none;
}

input,
button {
	outline: none;
}
