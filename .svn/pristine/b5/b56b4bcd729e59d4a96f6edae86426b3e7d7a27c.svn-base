/* 选集 */
.bry_vidoBox_FilmList{
	float: left;
	margin-left: 3px;
	width: 221px;
	height: 100%;
}
.bry_vidoBox_FilmLi{
	width: 100%;
	padding-top: 15px;
	box-sizing: border-box;
	height: calc(464px / 6);
	background: #017289;
	margin-bottom: 2px;
	/* border-bottom: 2px solid #002830; */
	cursor: pointer;
}
.bry_vidoBox_FilmLi>span{
	display: block;
	width: 221px;
	text-align: center;
	color: #FFFFFF;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}
.bry_vidoBox_FilmLi>span:nth-of-type(1){
	font-size: 12px;
}
.bry_vidoBox_FilmLi>span:nth-of-type(2){
	margin-top: 7px;
	font-size: 14px;
}
