/* 视频播放器盒子 */
.bry_vidoBox_playsBox{
	float: left;
	margin-left: 3px;
	width: 734px;
	height: 100%;
	cursor: pointer;
}
.bry_vidoBox_playsBox>img{
	width: 100%;
	height: 100%;
}
.bry_vidoBox_playsBox_down{
	width: 100%;
	height: 62px;
}

