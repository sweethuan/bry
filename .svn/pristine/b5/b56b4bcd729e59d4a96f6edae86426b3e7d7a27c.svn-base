/* 推存视频 */
.bry_activity_video_newest {
	width: 100%;
	padding-top: 30px ;
	box-sizing: border-box;
}

.video_recommend_list_item {
	font-size: 20px;
	float: left;
	padding:0 50px;
	color: #02a0c0;
	cursor: pointer;
	box-sizing: border-box;
}

.color_pink {
	color: #e15f5f;
}

/* 视频 */
.recommend_video {
	width: 100%;
	height: 128px;
	padding: 54px 0;
}

.recommend_video .swiper-container {
	width: 100%;
	height: 100%;
	padding: 0 36px;
	box-sizing: border-box;
}

.swiper-slide {
	text-align: center;
	font-size: 18px;
	/* Center slide text vertically */
	display: -webkit-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-ms-flex-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-box-align: center;
	-ms-flex-align: center;
	-webkit-align-items: center;
	align-items: center;
}

.swiper-button-next {
	color: #92854e;
	position: absolute;
	right: 0;
}

.swiper-button-prev {
	color: #92854e;
	position: absolute;
	left: 0;
}
.recommend_video .swiper-slide{
	width: 225px;
	height: 145px;
	margin-right: 32px
}
.swiper_div {
	width: 225px;
	height: 127px;
	overflow: hidden;
}
.recommend_video .swiper_div img{
	width: 100%;
	height: 100%;
	overflow: hidden;
}

/* 精选视频 */
.bry_activity_featured_videos {
	padding: 50px 19px;
	box-sizing: border-box;
} 
