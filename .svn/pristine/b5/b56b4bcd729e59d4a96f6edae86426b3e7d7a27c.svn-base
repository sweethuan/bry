/* 背景颜色 */
body {
	width: 100%;
	background: #002830;
}

#pointPic{
	width: 333px;
	height: 475px;
}

/* 海报图片 */
.bry_vidoBox_picture{
	float: left;
	width: 333px;
	height: 100%;
}
.bry_vidoBox_picture img{
	width: 100%;
	height: 100%;
}
.swiper-slide {
	text-align: center;
	font-size: 18px;
	/* Center slide text vertically */
	display: -webkit-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-ms-flex-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-box-align: center;
	-ms-flex-align: center;
	-webkit-align-items: center;
	align-items: center;
}
.bry_vidoBox_picture img{
	width: 100%;
}
.bry_vidoBox_picture .picture_button{
	color: #7b93b3;
}

/* 焦点 */
.bry_activity_focus {
	height: 500px;
	background: #e15f5f;
}

.focus_content {
	width: 100%;
	height: 100%;
	padding: 12px 12px 14px 12px;
	box-sizing: border-box;
	overflow: hidden;
}

.bry_activity_focus_img {
	width: 333px;
	height: 475px;
	overflow: hidden;
	float: left;
}
.bry_activity_focus_img .swiper-container{
	width: 100%;
}
.bry_activity_focus_img .swiper-slide{
	width: 100%;
}
.bry_open_focus_img img {
	width: 100%;
}


