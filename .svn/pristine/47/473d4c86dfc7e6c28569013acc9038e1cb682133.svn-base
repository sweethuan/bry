/* 跳页部分 */
.bry_main_JumpPage{
	/* margin: 55px 0 25px 0; */
	width: 100%;
	height: 30px;
	display: flex;
	justify-content: flex-end;
}

.bry_main_JumpPage_listType1{
	font-size: 14px;
	color: #02c6d5;
	line-height: 32px;
	cursor: pointer;
	margin: 0 4px;
}


input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
	-webkit-appearance: none;
}

input,
button {
	outline: none;
}